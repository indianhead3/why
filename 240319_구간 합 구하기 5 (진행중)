#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int a[1025][1025];
int psum[1025][1025];

int main()
{
	freopen("input.txt", "r", stdin);
	int n, m;
	cin >> n >> m;

	for (int x = 1; x <= n; x++) {
		for (int y = 1; y <= n; y++) {
			cin >> a[x][y];
			psum[x][y] = psum[x - 1][y] - psum[x][y - 1] - psum[x - 1][y - 1] + a[x][y];
		}
	}

	int x1, x2, y1, y2;
	cin >> x1 >> y1 >> x2 >> y2;

	for (int i = 0; i < m; i++) {
		int sum = 0;
		sum = psum[x2][y2] - psum[x2-1][y1-1] - psum[x1-1][y2-1] + psum[x1-1][y1-1];
		cout << sum << endl;
	}


	return 0;
}
